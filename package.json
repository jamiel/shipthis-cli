{
  "name": "shipthis",
  "description": "Mobile Game Shipping Tool",
  "version": "0.0.0",
  "author": "Hello Invent Ltd",
  "bin": {
    "shipthis": "./bin/run.js"
  },
  "bugs": "https://github.com/oclif-cli/shipthis/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "axios": "^1.7.7",
    "fs-extra": "^11.2.0",
    "ink": "^5.0.1",
    "react": "^18.3.1",
    "tough-cookie": "^4.1.4",
    "tsc-alias": "^1.8.10"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/react": "^18.3.5",
    "@types/tough-cookie": "^4.0.5",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/oclif-cli/shipthis",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "shipthis",
    "dirname": "shipthis",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "oclif-cli/shipthis",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "build-new": "shx rm -rf dist && npx tsc --build && npx tsc-alias -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
